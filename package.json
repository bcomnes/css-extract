{
  "name": "css-extract",
  "version": "1.0.1",
  "description": "Extract CSS from a browserify bundle",
  "main": "index.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "test": "standard && npm run deps && NODE_ENV=test node test",
    "test:cov": "standard && npm run deps && NODE_ENV=test istanbul cover ./test/index.js"
  },
  "repository": "stackcss/css-extract",
  "keywords": [
    "css",
    "extract",
    "browserify",
    "plugin",
    "transform",
    "sheetify",
    "css-modules"
  ],
  "license": "MIT",
  "dependencies": {
    "falafel": "^1.2.0",
    "is-require": "0.0.1",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "bl": "^1.1.2",
    "browserify": "^13.0.0",
    "dependency-check": "^2.5.1",
    "insert-css": "^0.2.0",
    "istanbul": "^0.4.2",
    "sheetify": "^4.1.0",
    "standard": "^6.0.7",
    "tape": "^4.5.0"
  },
  "files": [
    "index.js",
    "bin/*"
  ]
}
